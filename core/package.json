{
  "name": "@aldegad/nuxt-core",
  "description": "Core package for My Nuxt Projects",
  "version": "0.0.2",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/aldegad/nuxt-core.git"
  },
  "main": "./dist/nuxt-core.umd.cjs",
  "module": "./dist/nuxt-core.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/nuxt-core.js",
      "require": "./dist/nuxt-core.umd.cjs",
      "types": "./dist/core/src/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "eslint": "npx eslint .",
    "tsc": "npx tsc --noEmit",
    "prettier": "prettier --write .",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "build:js": "vite build --config vite.lib.config.ts",
    "build:types": "tsc --project tsconfig.build.json",
    "build:lib": "yarn build:js && yarn build:types"
  },
  "dependencies": {
    "@nuxt/eslint": "1.5.2",
    "@nuxt/test-utils": "3.19.2",
    "@pinia/nuxt": "^0.11.1",
    "@tailwindcss/vite": "^4.1.11",
    "eslint": "^9.0.0",
    "nuxt": "^3.17.7",
    "overlayscrollbars": "^2.11.4",
    "overlayscrollbars-vue": "^0.5.9",
    "pinia": "^3.0.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
